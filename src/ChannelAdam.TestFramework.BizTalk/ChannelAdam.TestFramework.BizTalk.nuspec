  <files>
    <file src="bin\Release\**\ChannelAdam.TestFramework.BizTalk.dll" target="lib" />
    <file src="bin\Release\**\ChannelAdam.TestFramework.BizTalk.pdb" target="lib" />
    <file src="**\*.cs" exclude="obj\**\*;bin\**\*" target="src" />
  </files>
